<Window x:Class="Mini_Projekt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mini_Projekt"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="450">
    <Grid Margin="-3,0,3.333,-0.333" Background="#FF874646">
        <TextBox x:Name="input_pesel" HorizontalAlignment="Left" Height="40" Margin="148,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="148" />
        <Label Content="Wpisz swój pesel:" HorizontalAlignment="Left" Margin="37,70,0,0" VerticalAlignment="Top" Height="45" Width="106" Foreground="White"/>
        <Button Content="Pokaż dane" HorizontalAlignment="Left" Height="30" Margin="148,142,0,0" VerticalAlignment="Top" Width="148" Click="Btn_Click"/>
        <ListBox x:Name="lb1" HorizontalAlignment="Left" Height="160" Margin="19,228,0,0" VerticalAlignment="Top" Width="401"/>
        <Label Content="Analiza Peselu"   HorizontalAlignment="center" Margin="0,0,0,0" VerticalAlignment="top" Width="148" Foreground="White" FontWeight="Bold" Height="44" FontSize="20" FontStyle="Italic"/>
        <Label Content="Dane:" HorizontalAlignment="Left" Margin="19,197,0,0" VerticalAlignment="Top" Width="113" Foreground="White"/>

    </Grid>
</Window>
